	"version": "0.2.0",
	"configurations": [{
		"name": "PowerShell Launch Current File",
		"type": "PowerShell",
		"request": "launch",
		"script": "${file}",
		"cwd": "${workspaceFolder}"
	}, {
		"name": "Build",
		"type": "PowerShell",
		"request": "launch",
		"script": "${workspaceFolder}/build.ps1",
		"cwd": "${workspaceFolder}"
	}, {
		"name": "Create labels",
		"type": "PowerShell",
		"request": "launch",
		"script": "${workspaceFolder}/scripts/create-labels.ps1",
		"cwd": "${workspaceFolder}"
	}, {
		"name": "Merge labels",
		"type": "PowerShell",
		"request": "launch",
		"script": "${workspaceFolder}/scripts/merge-labels.ps1",
		"cwd": "${workspaceFolder}"
	}]
}